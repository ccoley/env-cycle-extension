{
    "manifest_version": 2,
    "name": "Env Switch",
    "description": "This extension switches the current tab between the three development environments.",
    "version": "0.1",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Cycle between environments"
    },
    "background": {
        "scripts" : ["cycle.js"],
        "persistent": false
    }
}
